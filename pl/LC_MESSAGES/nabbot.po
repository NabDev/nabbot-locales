msgstr[3] ""

#: utils/pages.py:356 utils/pages.py:420
msgid "No help given."
msgstr "Brak pomocy."

#: utils/pages.py:414
#, python-brace-format
msgid "Use \"{prefix}help «command»\" for more info on a command."
msgstr "Użyj \"{prefix}help «komenda»\" aby otrzymać więcej informacji na temat danej komendy."

#: utils/pages.py:424
#, python-format
msgid "%d command"
msgid_plural "%d commands"
msgstr[0] "%d komenda"
msgstr[1] "%d komendy"
